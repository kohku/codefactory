	<uploadStorageSettings defaultProvider="LinqUploadStorageProvider">
		<providers>
			<add name="LinqUploadStorageProvider"
           type="CodeFactory.Web.Storage.LinqUploadStorageProvider, CodeFactory.Web"
           applicationName="CodeFactory.Gallery.WebClient"
           connectionStringName="SqlServices"
           maxFileSize="8192"
           saveContentType="application/pdf;
                            image/pjpeg;
                            image/jpeg;
                            image/gif;
                            image/bmp;
                            image/x-png;
                            text/plain"
           compressContentType="text/plain"/>
