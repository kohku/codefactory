           saveContentType="application/pdf;
                            image/pjpeg;
                            image/jpeg;
                            image/gif;
                            image/bmp;
                            image/x-png;
                            text/plain"
           compressContentType="text/plain"/>
		</providers>
		<thumbnail quality="true" width="154" height="113" noThumbnailAvailable="images/noThumbnailAvailable.jpg"/>
