		</generalSettings>
		<galleryManagementSettings defaultProvider="LinqGalleryProvider">
			<providers>
				<clear/>
				<add name="DbGalleryProvider"
             type="CodeFactory.Gallery.Core.Providers.DbGalleryProvider"
             applicationName="CodeFactory.Gallery.WebClient"/>
        <add name="LinqGalleryProvider"
