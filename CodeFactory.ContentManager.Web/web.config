      <forms loginUrl="~/authentication/login.aspx"/>
    </authentication>
    <authorization>
      <allow users="*"/>
    </authorization>
    <customErrors mode="Off" defaultRedirect="~/GenericErrorPage.aspx">
      <error statusCode="403" redirect="~/NoAccess.aspx"/>
      <error statusCode="404" redirect="~/FileNotFound.aspx"/>
    </customErrors>
    <httpHandlers>
      <remove verb="*" path="*.asmx"/>
      <add verb="*" path="*.asmx" validate="false" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      <add verb="*" path="image.axd" type="CodeFactory.Web.HttpHandlers.UploadStorageImageHandler, CodeFactory.Web" validate="false"/>
      <add verb="*" path="file.axd" type="CodeFactory.Web.HttpHandlers.UploadStorageFileHandler, CodeFactory.Web" validate="false"/>
      <add verb="*" path="*_AppService.axd" validate="false" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      <add verb="GET,HEAD" path="ScriptResource.axd" type="System.Web.Handlers.ScriptResourceHandler, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" validate="false"/>
      <add verb="*" path="resizeImage.axd" type="CodeFactory.Web.HttpHandlers.ResizeImage, CodeFactory.Web" validate="false" />
    </httpHandlers>
    <httpModules>
      <add name="UrlRewrite" type="CodeFactory.ContentManager.HttpModules.UrlRewrite, CodeFactory.ContentManager"/>
