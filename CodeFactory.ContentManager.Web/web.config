      <providers>
        <clear/>
        <add name="XmlSiteMapProvider" type="System.Web.XmlSiteMapProvider, System.Web, Version=2.0.3600.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" siteMapFile="Web.sitemap" securityTrimmingEnabled="true"/>
        <add name="ContentManagerProvider" type="CodeFactory.ContentManager.Providers.SiteMapProvider, CodeFactory.ContentManager" securityTrimmingEnabled="true"/>
      </providers>
    </siteMap>
    <webParts>
      <personalization defaultProvider="FileBasedProvider">
        <providers>
          <clear/>
          <add connectionStringName="SqlServices" name="SqlPersonalizationProvider" type="CodeFactory.ContentManager.WebControls.WebParts.PersonalizationProvider, CodeFactory.ContentManager" applicationName="CodeFactory.ContentManager.Web"/>
          <add name="FileBasedProvider" type="CodeFactory.ContentManager.WebControls.WebParts.FileBasedPersonalizationProvider, CodeFactory.ContentManager" applicationName="CodeFactory.ContentManager.Web" directoryName="~/App_Data/Files"/>
        </providers>
        <authorization>
          <deny users="?" verbs="enterSharedScope"/>
          <deny users="?" verbs="modifyState"/>
          <allow users="*" verbs="enterSharedScope"/>
          <allow users="*" verbs="modifyState"/>
        </authorization>
      </personalization>
    </webParts>
